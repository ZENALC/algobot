[FORMAT]
max-line-length = 120

[MESSAGES CONTROL]
disable=
    too-many-instance-attributes,
    len-as-condition,
    too-few-public-methods,
    anomalous-backslash-in-string,
    no-else-return,
    simplifiable-if-statement,
    too-many-arguments,
    duplicate-code,
    no-name-in-module,
    no-member,
    print-statement,
    parameter-unpacking,
    unpacking-in-except,
    old-raise-syntax,
    backtick,
    long-suffix,
    old-ne-operator,
    old-octal-literal,
    import-star-module-level,
    raw-checker-failed,
    bad-inline-option,
    locally-disabled,
    locally-enabled,
    file-ignored,
    suppressed-message,
    useless-suppression,
    deprecated-pragma,
    apply-builtin,
    basestring-builtin,
    buffer-builtin,
    cmp-builtin,
    coerce-builtin,
    execfile-builtin,
    file-builtin,
    long-builtin,
    raw_input-builtin,
    reduce-builtin,
    standarderror-builtin,
    unicode-builtin,
    xrange-builtin,
    coerce-method,
    delslice-method,
    getslice-method,
    setslice-method,
    no-absolute-import,
    old-division,
    dict-iter-method,
    dict-view-method,
    next-method-called,
    metaclass-assignment,
    indexing-exception,
    raising-string,
    reload-builtin,
    oct-method,
    hex-method,
    nonzero-method,
    cmp-method,
    input-builtin,
    round-builtin,
    intern-builtin,
    unichr-builtin,
    map-builtin-not-iterating,
    zip-builtin-not-iterating,
    range-builtin-not-iterating,
    filter-builtin-not-iterating,
    using-cmp-argument,
    eq-without-hash,
    div-method,
    idiv-method,
    rdiv-method,
    exception-message-attribute,
    invalid-str-codec,
    sys-max-int,
    bad-python3-import,
    deprecated-string-function,
    deprecated-str-translate-call,
    import-error,
    missing-docstring,
    invalid-name,
    bad-whitespace,
    consider-using-enumerate,
    unidiomatic-typecheck,
    cyclic-import,
    broad-except,
    no-else-break,
    super-with-arguments,
    consider-merging-isinstance,
    protected-access,
    unnecessary-lambda,
    unused-argument,
    fixme,
    redefined-argument-from-local,
    consider-using-with,
    arguments-differ,
    too-many-public-methods,
    simplifiable-if-expression,
    consider-iterating-dictionary,
    simplifiable-condition,
    too-many-locals,
    too-many-nested-blocks,
    arguments-renamed,
    inconsistent-return-statements,
    unused-variable,
    unsubscriptable-object,
    too-many-return-statements,
    unnecessary-pass,
    no-self-use,
    consider-using-dict-items,
    raise-missing-from,
    consider-using-max-builtin,
    unnecessary-comprehension,
    too-many-branches,
    consider-using-from-import,
    unused-wildcard-import,
    expression-not-assigned,
    too-many-statements,
    wildcard-import,
    too-many-lines
